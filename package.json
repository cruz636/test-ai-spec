{
    "name": "auth-system",
    "version": "1.0.0",
    "description": "User authentication system with TypeScript and Node.js",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "createsuperuser": "ts-node src/commands/createSuperUser.ts",
        "changepassword": "ts-node src/commands/changePassword.ts",
        "upgradetosuperuser": "ts-node src/commands/upgradeToSuperuser.ts",
        "generate-report": "ts-node src/commands/generateProjectReport.ts"
    },
    "keywords": [
        "authentication",
        "typescript",
        "nodejs",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.500.0",
        "@aws-sdk/s3-request-presigner": "^3.500.0",
        "bcryptjs": "^2.4.3",
        "commander": "^11.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "nanoid": "^3.3.7",
        "nodemailer": "^6.9.7",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.4",
        "@types/nodemailer": "^6.4.14",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint": "^8.55.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}